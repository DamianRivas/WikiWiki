<h2>All Wikis</h2>
<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |wiki| %>
      <p><%= link_to wiki.title, wiki_path(wiki.id) %></p>
    <% end %>
  </div>
  <% if current_user %>
    <div class="col-md-4">
      <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success pull-right' %>
    </div>
  <% end %>
</div>
